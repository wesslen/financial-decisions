    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "file-type": "^16.0.1",
    "papaparse": "^5.3.0",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.24",
    "nodemon": "^2.0.6",
    "randomstring": "^1.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "survey-react": "^1.8.17"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
